       "def timed_func(func_to_time):\n",
       "    def timed(*args, **kwargs):\n",
       "        start = time.perf_counter()\n",
       "        res = func_to_time(*args, **kwargs)\n",
       "        print(time.perf_counter() - start)\n",
       "        return res\n",
       "    return timed"
      ]
